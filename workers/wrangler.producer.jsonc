{
    "name": "gtm-producer",
    "main": "gtm-producer.ts",
    "compatibility_date": "2025-12-24",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "queues": {
        "producers": [
            {
                "queue": "gtm-events-queue",
                "binding": "GTM_QUEUE"
            }
        ]
    },
    "vars": {
        "ENVIRONMENT": "production"
    }
}